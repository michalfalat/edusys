  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": "0ba1b3e0-4bcc-4f21-bffd-8c2e2c007f41"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
